{
    "manifest_version": 3,
    "name": "Freedium for Medium",
    "version": "1.0",
    "description": "Adds a context menu to open Medium articles through Freedium, bypassing the paywall.",
    "permissions": [
      "contextMenus",
      "tabs"
    ],
    "host_permissions": [
      "*://medium.com/*",
      "https://freedium.cfd/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  